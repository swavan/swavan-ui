{
  "manifest_version": 2,
  "name": "SwaVan",
  "homepage_url": "https://swavan.io",
  "description": "SwaVan allow user to Intercept and forward XHR request to another endpoint, User can add forward URL or data into the application, All data are store locally.",
  "default_locale": "en",
  "permissions": [
    "<all_urls>",
    "webRequest",
    "webRequestBlocking"
  ],
  "icons": {
    "16": "icons/SwaVan.png",
    "32": "icons/SwaVan.png",
    "64": "icons/SwaVan.png",
    "128": "icons/SwaVan.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Swavan",
    "default_icon": {
      "19": "icons/SwaVan.png",
      "38": "icons/SwaVan.png"
    }
  }
}
